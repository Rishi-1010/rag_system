{
    "name": "rag-system/backend",
    "description": "Backend for RAG System",
    "type": "project",
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.0",
        "monolog/monolog": "^3.0",
        "elasticsearch/elasticsearch": "^8.17",
        "openai-php/client": "^0.10.3",
        "symfony/dotenv": "^6.0",
        "symfony/console": "^6.0",
        "smalot/pdfparser": "^2.12"
    },
    "autoload": {
        "psr-4": {
            "Rag\\": "rag/"
        }
    },
    "authors": [
        {
            "name": "RAG System",
            "email": "admin@rag.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "php-http/discovery": true
        }
    }
}
